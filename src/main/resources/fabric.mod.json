{
  "schemaVersion": 1,
  "id": "woods_and_mires",
  "version": "${version}",

  "name": "Woods and Mires",
  "description": "Adds some new biomes based on Nordic nature.",
  "authors": ["Juuz"],

  "license": "LGPL-3.0-only",
  "icon": "assets/woods_and_mires/icon.png",
  "contact": {
    "homepage": "https://github.com/Juuxel/WoodsAndMires",
    "sources": "https://github.com/Juuxel/WoodsAndMires",
    "issues": "https://github.com/Juuxel/WoodsAndMires/issues"
  },

  "environment": "*",
  "entrypoints": {
    "main": [
      "juuxel.woodsandmires.WoodsAndMires"
    ],
    "client": [
      "juuxel.woodsandmires.client.WoodsAndMiresClient"
    ],
    "terrablender": [
      "juuxel.woodsandmires.terrablender.WoodsAndMiresTb"
    ]
  },
  "accessWidener": "woods_and_mires.accesswidener",
  "mixins": [
    "mixins.woods_and_mires.json",
    {
      "config": "mixins.woods_and_mires.client.json",
      "environment": "client"
    }
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": ">=0.75.1",
    "fabric-biome-api-v1": "^9.1.0",
    "minecraft": "=1.19.2"
  },
  "recommends": {
    "terrablender": "~2.0"
  }
}
